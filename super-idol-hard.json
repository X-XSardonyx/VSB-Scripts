sp:SetNetworkOwner(owner)
sp.Transparency = 1
sp.CanCollide = false

sound.SoundId = "rbxassetid://9045344148"
sound.MaxDistance = 50
sound.Looped = true
sound:Play()

for i = 1,20 do
local frame = Instance.new("Frame", bottom)
frame.Position = UDim2.new(.5,-math.sin(i)*300,.5,math.cos(i)*300)
frame.Rotation = 360/i
frame.Size = UDim2.new(0,50,0,100)
coroutine.wrap(function()
while wait() do
frame.Size = UDim2.new(0,50,0,math.sin(math.noise(i,vol))*30+20)
end
end)()
end

while true do
task.wait()
vol = sp.Position.X

end