function joke()
task.wait()
jokee = game:service("HttpService"):JSONDecode(game:service("HttpService"):GetAsync("https://v2.jokeapi.dev/joke/Any?safe-mode"))


if jokee.error then
warn("error")
return joke()
end

if jokee.type then
if jokee.type ~= "single" then
warn(jokee.type)
return joke()
end
end

return jokee.joke

end

